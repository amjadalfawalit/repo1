<?xml version="1.0" encoding="utf-8"?>
<odoo>



    <record id="view_stock_location_type_tree" model="ir.ui.view">
        <field name="name">location.type.tree</field>
        <field name="model">location.type</field>
        <field name="arch" type="xml">
            <tree string="Location Types" editable="bottom" class="o_employee_form">
                <field name="name"/>
                <field name="price"/>
                <field name="warehouse_id"></field>
            </tree>
        </field>
    </record>


    <record id="action_stock_location_type" model="ir.actions.act_window">
        <field name="name">Location Type</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">location.type</field>
        <field name="view_mode">tree</field>
        <field name="context">{}</field>
    </record>


</odoo>
 
